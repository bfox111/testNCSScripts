{
    "tests": [
            {
          "testName": "Delete VLINE ",
          "testDescription": "Delete VLINE test between PE-6x and PE-10x.",
          "testType": "SAT",

          "customScripts": [
              {
                "deviceName": "PE-6x",
                "cmdFile": "DeleteVLINE",
                "scriptAttributes": {
                  "SVLAN": "50",
                  "NNI_PORT": "11",
                  "UNI_PORT": "10",
                  "SERVICE_NUM": "1"
              }
              },
                            {
                "deviceName": "PE-10x",
                "cmdFile": "DeleteVLINE",
                "scriptAttributes": {
                  "UNI_PORT": "1",
                  "SVLAN": "50",
                  "MBPS_CIR": "1000",
                  "CIRCUIT_NAME": "VLINE50",
                  "NNI_PORT": "4"
              }
              }

          ],
          "sleepTime": 5,
          "verificationCommands": {
              "PE-6x": [
                  "vlan show",
                  "traffic-prof stand show"
              ],
              "PE-10x": [
                  "show flow",
                  "show meter-profiles",
                  "config compare source before target running"
              ]
          }
      },
