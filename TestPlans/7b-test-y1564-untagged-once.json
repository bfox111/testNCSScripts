{
    "tests": [
        {
          "testName": "Y1564 Test",
          "testDescription": "Setup Y.1564 test between PE-6x and PE-10x.  PE-10x is the reflector.",
          "testType": "SAT",

          "customScripts": [
              {
                "deviceName": "PE-10x",
                "cmdFile": "ReflectTaggedAndUntagged",
                "scriptAttributes": {
                  "UNI_PORT": "2",
                  "FLOW_POINT": "FP_2_ELINE610"
                }
              },
              {
                "deviceName": "PE-6x",
                "cmdFile": "Y1564SetupOnceUntagged",
                "scriptAttributes": {
                  "UNI_PORT": "2",
                  "BANDWIDTH_MBS": "1000",
                  "DEST_NODE": "PE-10x",
                  "DEST_MAC": "SET_IN_CODE"
                }
              }
          ],
          "sleepTime": 15,
          "verificationCommands": {
               "PE-6x": [
                  "bench test show"
              ],
              "PE-10x": [
                  "show sat entities"
              ]
          }
      },
              {
          "testName": "Start Y1564 Test",
          "testDescription": "Start Y.1564 test between PE-6x and PE-10x.  PE-10x is the reflector.",
          "testType": "SAT",

          "customScripts": [
              {
                "deviceName": "PE-6x",
                "cmdFile": "Y1564StartUntagged",
                "scriptAttributes": {
              }
              }
          ],
          "sleepTime": 90,
          "verificationCommands": {
              "PE-10x": [
                  "sh sat test-instances statistics"
              ],
              "PE-6x": [
                  "bench test show name genY1564Untagged statistics"
              ]
          }
      },
                    {
          "testName": "Start Y1564 Test A SECOND TIME",
          "testDescription": "Start Y.1564 test between PE-6x and PE-10x.  PE-10x is the reflector.",
          "testType": "SAT",

          "customScripts": [
              {
                "deviceName": "PE-6x",
                "cmdFile": "Y1564StartUntagged",
                "scriptAttributes": {
              }
              }
          ],
          "sleepTime": 90,
          "verificationCommands": {
              "PE-10x": [
                  "sh sat test-instances statistics"
              ],
              "PE-6x": [
                  "bench test show name genY1564Untagged statistics"
              ]
          }
      }

     
    ]
